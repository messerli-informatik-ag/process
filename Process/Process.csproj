        <RepositoryUrl>https://github.com/messerli-informatik-ag/process</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>TODO</PackageTags>
        <PackageLicenseExpression>MIT OR Apache-2.0</PackageLicenseExpression>
        <Copyright>Â© Messerli Informatik AG. All rights reserved.</Copyright>
        <Description>TODO</Description>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Funcky" />
        <PackageReference Include="System.Collections.Immutable" Condition="'$(TargetFramework)' == 'netstandard2.1'" />
    </ItemGroup>
</Project>
